  "scripts": {
    "sync": "cd ..; node sync.js; cd sris",
    "dev": "npm run sync && docker-compose up -d --build && concurrently \"npm run generate-api &&NODE_ENV=local ts-node --project tsconfig.server.json src/monoCommon/server/index.ts\"",
    "dev:lint": "concurrently \"npm run dev\" \"npm run lint:watch\"",
    "dev:next": "next dev",
    "clear": "docker-compose rm -s -f",
    "test": "SAVIVALDYBES_GEOSERVER=http://geo.biip.lt QGIS=http://gis.biip.lt ENV=test jest",
    "test:watch": "ENV=test npx jest --watch",
    "build:server": "npm run generate-api && tsc --project tsconfig.server.json",
    "build:next": "npm run generate-api && next build",
    "build": "npm run build:next",
    "start": "PROD=true ts-node --project tsconfig.server.json src/monoCommon/server/index.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:watch": "watch 'npm run lint' ./src",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "generate-api": "npm run generate-flow && ts-node --project tsconfig.server.json   src/monoCommon/generateApi.ts",
    "generate-flow": "ts-node --project tsconfig.server.json   src/BPMN/generateFlow.ts",
    "updateSubTree": "git subtree pull --prefix JSMD https://github.com/tkukis/JSMD.git dmz --squash"
  },
